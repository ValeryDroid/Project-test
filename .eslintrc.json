{

    "env": {
  
      "browser": true,
  
      "es2021": true
  
    },

      "extends": [
    
        "eslint:recommended",
    
        "plugin:prettier/recommended"
    
      ],
  
    "rules": {
  
      "indent": ["error", 2],
  
      "quotes": ["error", "single"],
  
      "semi": ["error", "always"]
  
    }
  
}